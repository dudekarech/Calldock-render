{
  "name": "calldocker-backend",
  "version": "1.0.0",
  "description": "CallDocker - Web-based Call Center Management System",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "db:migrate": "psql -d calldocker_dev -f database/schema.sql",
    "db:seed": "node database/seed.js",
    "db:reset": "npm run db:drop && npm run db:create && npm run db:migrate && npm run db:seed",
    "db:create": "createdb calldocker_dev",
    "db:drop": "dropdb calldocker_dev",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "keywords": [
    "call-center",
    "webrtc",
    "ivr",
    "customer-support",
    "real-time-communication"
  ],
  "author": "CallDocker Team",
  "license": "MIT",
  "dependencies": {
    "aws-sdk": "^2.1490.0",
    "bcrypt": "^5.1.1",
    "bull": "^4.12.0",
    "compression": "^1.7.4",
    "connect-pg-simple": "^8.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-session": "^1.17.3",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.7",
    "pg": "^8.11.3",
    "redis": "^4.6.10",
    "socket.io": "^4.7.4",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "twilio": "^4.19.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "eslint": "^8.54.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-n": "^16.3.1",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.0",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/calldocker/calldocker-backend.git"
  },
  "bugs": {
    "url": "https://github.com/calldocker/calldocker-backend/issues"
  },
  "homepage": "https://github.com/calldocker/calldocker-backend#readme"
}
